<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<!--
    Configuration file for UrlRewriteFilter
    http://www.tuckey.org/urlrewrite/
-->
<urlrewrite>

    <rule>
        <from>/index.html</from>
        <to>/seats.jsf</to>
    </rule>
    <outbound-rule>
        <from>/seats.jsf</from>
        <to>/index.html</to>
    </outbound-rule>

    <rule>
        <from>/party.html</from>
        <to>/party.jsf</to>
    </rule>
    <outbound-rule>
        <from>/party.jsf</from>
        <to>/party.html</to>
    </outbound-rule>

    <rule>
        <from>/constituency.html</from>
        <to>/constituency.jsf</to>
    </rule>
    <outbound-rule>
        <from>/constituency.jsf</from>
        <to>/constituency.html</to>
    </outbound-rule>

    <rule>
        <from>/district.html</from>
        <to>/district.jsf</to>
    </rule>
    <outbound-rule>
        <from>/district.jsf</from>
        <to>/district.html</to>
    </outbound-rule>

    <rule>
        <from>/admin.html</from>
        <to>/admin.jsf</to>
    </rule>
    <outbound-rule>
        <from>/admin.jsf</from>
        <to>/admin.html</to>
    </outbound-rule>

</urlrewrite>
